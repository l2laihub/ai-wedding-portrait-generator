# WedAI Environment Configuration
# Copy this file to .env.local and fill in your values

# Supabase Configuration (Client-side)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Legacy Gemini API Key (for fallback only)
# Note: The secure backend uses GEMINI_API_KEY as a Supabase secret
GEMINI_API_KEY=your-gemini-api-key-here

# PostHog Analytics (Optional)
VITE_POSTHOG_KEY=your-posthog-key-here
VITE_POSTHOG_HOST=https://app.posthog.com

# Stripe (for payments)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key-here

# Development Settings
NODE_ENV=development
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_STRIPE=true
VITE_ENABLE_SECURE_BACKEND=true

# Rate Limiting Configuration
VITE_RATE_LIMIT_ANONYMOUS_HOURLY=5
VITE_RATE_LIMIT_ANONYMOUS_DAILY=10
VITE_RATE_LIMIT_AUTHENTICATED_HOURLY=30
VITE_RATE_LIMIT_AUTHENTICATED_DAILY=100
VITE_RATE_LIMIT_PREMIUM_HOURLY=100
VITE_RATE_LIMIT_PREMIUM_DAILY=500